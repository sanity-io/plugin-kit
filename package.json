{
  "name": "@sanity/plugin-kit",
  "version": "1.0.1",
  "description": "Enhanced Sanity.io plugin development experience",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "node executable-index.js",
    "watch": "tsc --watch",
    "test": "tap",
    "prepare": "husky install",
    "commit": "git-cz",
    "lint": "eslint .",
    "compile": "tsc --noEmit",
    "format": "prettier src -w",
    "prepublishOnly": "npm run test && npm run build"
  },
  "binname": "sanity-plugin",
  "source": "./src/index.ts",
  "types": "./lib/src/index.d.ts",
  "bin": "lib/src/index.js",
  "main": "./lib/src/index.js",
  "files": [
    "v2-incompatible.js",
    "src",
    "lib",
    "assets"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/plugin-kit.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "sanity-io",
    "sanity",
    "plugin",
    "development",
    "babel",
    "typescript",
    "bootstrap"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/plugin-kit/issues"
  },
  "homepage": "https://github.com/sanity-io/plugin-kit#readme",
  "dependencies": {
    "@rexxars/choosealicense-list": "^1.1.2",
    "chalk": "^4.1.2",
    "concurrently": "^7.3.0",
    "discover-path": "^1.0.0",
    "email-validator": "^2.0.4",
    "execa": "^5.1.1",
    "find-babel-config": "^1.2.0",
    "get-it": "^6.1.1",
    "get-latest-version": "^4.0.0",
    "git-remote-origin-url": "^3.1.0",
    "git-user-info": "^1.0.1",
    "github-url-to-object": "^4.0.6",
    "inquirer": "^8.2.4",
    "meow": "^10.1.3",
    "nodemon": "^2.0.19",
    "npm-packlist": "^3.0.0",
    "npm-run-path": "^4.0.1",
    "outdent": "^0.8.0",
    "p-any": "^3.0.0",
    "p-props": "^4.0.0",
    "postcss": "^8.4.16",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "spdx-license-ids": "^3.0.12",
    "validate-npm-package-name": "^3.0.0",
    "xdg-basedir": "^4.0.0",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@sanity/semantic-release-preset": "^2.0.0",
    "@types/eslint": "^8.4.6",
    "@types/inquirer": "^8.2.3",
    "@types/node": "^17.0.40",
    "@types/nodemon": "^1.19.2",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^5.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "json5": "^2.2.1",
    "lint-staged": "^12.5.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "readdirp": "^3.6.0",
    "rimraf": "^3.0.2",
    "sanity": "3.0.0-dev-preview.15",
    "sinon": "^14.0.0",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "tap": {
    "jobs": 2,
    "browser": false,
    "timeout": 120,
    "reporter": "spec",
    "check-coverage": false,
    "coverage-report": [
      "html"
    ],
    "test-ignore": "^lib/.*|.*ignore.*|.*run-test-command.*|.*fixture.utils.*",
    "ts": true
  }
}
