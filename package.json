{
  "name": "@sanity/plugin-kit",
  "version": "3.1.7",
  "description": "Enhanced Sanity.io plugin development experience",
  "keywords": [
    "sanity-io",
    "sanity",
    "plugin",
    "development",
    "typescript",
    "bootstrap"
  ],
  "homepage": "https://github.com/sanity-io/plugin-kit#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/plugin-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/plugin-kit.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "bin": {
    "plugin-kit": "./bin/plugin-kit.js"
  },
  "files": [
    "assets",
    "bin",
    "dist",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "commit": "git-cz",
    "compile": "tsc --build",
    "format": "prettier src -w",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "run-s build",
    "test": "tap",
    "watch": "pkg-utils watch --strict"
  },
  "tap": {
    "browser": false,
    "check-coverage": false,
    "coverage-report": [
      "html"
    ],
    "jobs": 2,
    "reporter": "spec",
    "test-ignore": "^dist/.*|.*ignore.*|.*run-test-command.*|.*fixture.utils.*",
    "timeout": 120,
    "ts": true
  },
  "dependencies": {
    "@rexxars/choosealicense-list": "^1.1.2",
    "@sanity/pkg-utils": "^2.2.13",
    "chalk": "^4.1.2",
    "concurrently": "^8.0.0",
    "discover-path": "^1.0.0",
    "email-validator": "^2.0.4",
    "execa": "^5.1.1",
    "get-it": "^8.0.11",
    "get-latest-version": "^5.0.1",
    "git-remote-origin-url": "^3.1.0",
    "git-user-info": "^2.0.3",
    "github-url-to-object": "^4.0.6",
    "inquirer": "^8.2.5",
    "meow": "^9.0.0",
    "nodemon": "^2.0.21",
    "npm-packlist": "^7.0.4",
    "npm-run-path": "^4.0.1",
    "outdent": "^0.8.0",
    "p-any": "^3.0.0",
    "p-props": "^4.0.0",
    "postcss": "^8.4.21",
    "rimraf": "^4.4.0",
    "semver": "^7.3.8",
    "spdx-license-ids": "^3.0.13",
    "validate-npm-package-name": "^5.0.0",
    "xdg-basedir": "^4.0.0",
    "yalc": "^1.0.0-pre.53"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@sanity/semantic-release-preset": "^4.0.0",
    "@sanity/ui-workshop": "^1.2.1",
    "@types/eslint": "^8.21.1",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.4",
    "@types/nodemon": "^1.19.2",
    "@types/tap": "^15.0.8",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "json5": "^2.2.3",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prettier-plugin-packagejson": "^2.4.3",
    "readdirp": "^3.6.0",
    "rimraf": "^4.4.0",
    "sanity": "^3.3.1",
    "sinon": "^15.0.2",
    "tap": "^16.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "binname": "sanity-plugin"
}
