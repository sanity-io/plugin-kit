{
  "name": "@sanity/plugin-kit",
  "version": "2.1.11",
  "description": "Enhanced Sanity.io plugin development experience",
  "keywords": [
    "sanity-io",
    "sanity",
    "plugin",
    "development",
    "babel",
    "typescript",
    "bootstrap"
  ],
  "homepage": "https://github.com/sanity-io/plugin-kit#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/plugin-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/plugin-kit.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "main": "./lib/src/index.js",
  "source": "./src/index.ts",
  "types": "./lib/src/index.d.ts",
  "bin": "lib/src/index.js",
  "files": [
    "v2-incompatible.js",
    "src",
    "lib",
    "assets"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "node executable-index.js",
    "clean": "rimraf lib",
    "commit": "git-cz",
    "compile": "tsc --noEmit",
    "format": "prettier src -w",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "test": "tap",
    "watch": "tsc --watch"
  },
  "tap": {
    "browser": false,
    "check-coverage": false,
    "coverage-report": [
      "html"
    ],
    "jobs": 2,
    "reporter": "spec",
    "test-ignore": "^lib/.*|.*ignore.*|.*run-test-command.*|.*fixture.utils.*",
    "timeout": 120,
    "ts": true
  },
  "dependencies": {
    "@rexxars/choosealicense-list": "^1.1.2",
    "chalk": "^4.1.2",
    "concurrently": "^7.5.0",
    "discover-path": "^1.0.0",
    "email-validator": "^2.0.4",
    "execa": "^5.1.1",
    "find-babel-config": "^1.2.0",
    "get-it": "^6.1.1",
    "get-latest-version": "^4.0.0",
    "git-remote-origin-url": "^3.1.0",
    "git-user-info": "^1.0.1",
    "github-url-to-object": "^4.0.6",
    "inquirer": "^8.2.5",
    "meow": "^9.0.0",
    "nodemon": "^2.0.20",
    "npm-packlist": "^3.0.0",
    "npm-run-path": "^4.0.1",
    "outdent": "^0.8.0",
    "p-any": "^3.0.0",
    "p-props": "^4.0.0",
    "postcss": "^8.4.18",
    "rimraf": "^3.0.2",
    "semver": "^7.3.8",
    "spdx-license-ids": "^3.0.12",
    "validate-npm-package-name": "^3.0.0",
    "xdg-basedir": "^4.0.0",
    "yalc": "^1.0.0-pre.53"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@sanity/pkg-utils": "^1.17.2",
    "@sanity/semantic-release-preset": "^2.0.2",
    "@types/eslint": "^8.4.9",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.3",
    "@types/node": "^17.0.40",
    "@types/nodemon": "^1.19.2",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "json5": "^2.2.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "readdirp": "^3.6.0",
    "rimraf": "^3.0.2",
    "sanity": "3.0.0-rc.2",
    "sinon": "^14.0.0",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "binname": "sanity-plugin"
}
